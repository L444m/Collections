<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liamma.collections">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".main.ui.RecyclerActivity" />
        <activity android:name=".main.ui.StatusModeActivity" />
        <activity android:name=".main.ui.AdapterActivity" />
        <activity android:name=".main.ui.AnimatorActivity"/>
        <activity android:name=".main.ui.TestingActivity" />
        <activity android:name=".main.ui.DataBindingActivity" />
    </application>

</manifest>